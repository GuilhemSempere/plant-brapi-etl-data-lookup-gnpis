  "source-entity": "location",

  "document-transform": {
    "schema:url": "{.documentationURL}",
    "schema:includedInDataCatalog": "{.source}",

    "locationName": {
      "{or}": [
        "{.locationName}",
        "{.name}"
      ]
    },
    "instituteAddress": {
      "{or}": [

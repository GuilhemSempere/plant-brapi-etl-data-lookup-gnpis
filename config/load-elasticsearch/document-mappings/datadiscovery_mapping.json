{
  "datadiscovery": {
    "dynamic": false,
    "_source": {
      "includes": [
        "@id",
        "@type",
        "schema:*"
      ]
    },
    "properties": {
      "@type": {
        "type": "string",
        "index": "not_analyzed"
      },
      "@id": {
        "type": "string",
        "index": "not_analyzed"
      },
      "schema:includedInDataCatalog": {
        "type": "string",
        "index": "not_analyzed"
      },
      "schema:identifier": {
        "type": "string",
        "index": "not_analyzed"
      },
      "schema:name": {
        "type": "string",
        "index": "not_analyzed"
      },
      "schema:url": {
        "type": "string",
        "index": "not_analyzed"
      },
      "schema:description": {
        "type": "string",
        "index": "not_analyzed"
      },
      "germplasm": {
        "type": "object",
        "properties": {
          "cropName": {
            "type": "string",
            "index": "not_analyzed",
            "fields": {
              "suggest": {
                "type": "string",
                "index": "analyzed",
                "search_analyzer": "search_suggester",
                "analyzer": "index_suggester"
              }
            }
          },
          "germplasmList": {
            "type": "string",
            "index": "not_analyzed",
            "fields": {
              "suggest": {
                "type": "string",
                "index": "analyzed",
                "search_analyzer": "search_suggester",
                "analyzer": "index_suggester"
              }
            }
          },
          "accession": {
            "type": "string",
            "index": "not_analyzed",
            "fields": {
              "suggest": {
                "type": "string",
                "index": "analyzed",
                "search_analyzer": "search_suggester",
                "analyzer": "index_suggester"
              }
            }
          }
        }
      },
      "trait": {
        "type": "object",
        "properties": {
          "observationVariableIds": {
            "type": "string",
            "index": "not_analyzed"
          }
        }
      }
    }
  }
}
